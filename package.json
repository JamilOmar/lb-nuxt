{
  "name": "nuxt-ts-express",
  "version": "1.0.0",
  "description": "My incredible Nuxt.js project",
  "author": "Jamil Falconi",
  "private": true,
  "scripts": {
    "clean": "rimraf .nuxt dist-api *.tsbuildinfo .eslintcache",
    "dev": "npm run build-api && cross-env NODE_ENV=development nodemon --inspect server/index.ts --watch server",
    "build": "nuxt build",
    "start": "npm run build-api && cross-env NODE_ENV=production ts-node server/index.ts",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest",
    "build-api": "tsc -p tsconfig.api.json"
  },
  "dependencies": {
    "@loopback/boot": "^2.0.4",
    "@loopback/context": "^3.4.0",
    "@loopback/core": "^2.3.0",
    "@loopback/openapi-v3": "^3.1.3",
    "@loopback/repository": "^2.1.1",
    "@loopback/rest": "^3.2.1",
    "@loopback/rest-explorer": "^2.0.4",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "^1.4.0",
    "bootstrap": "^4.1.3",
    "bootstrap-vue": "^2.0.0",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "nuxt": "^2.15.8",
    "vue-property-decorator": "^8.4.1"
  },
  "devDependencies": {
    "@loopback/testlab": "^3.0.1",
    "@nuxt/types": "^0.7.4",
    "@nuxt/typescript-build": "^0.6.5",
    "@nuxt/typescript-runtime": "^0.4.5",
    "@nuxtjs/eslint-config-typescript": "^1.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^24.1.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.19.1",
    "ts-jest": "^25.0.0",
    "typescript": "^3.8.3",
    "vue-jest": "^4.0.0-0"
  }
}
